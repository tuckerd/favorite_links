<%= form_for @favorite do |f| -%>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url, 'URL' %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p>
  <%= f.submit %>
  <%= button_to 'Back', root_path %>
  </p> 
<% end -%>
